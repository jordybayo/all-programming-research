#:kivy 1.10.1


<Button>:
    font_size: 12
    color: 0,1,0,1

<FloatLayout>:
    Button:
        size_hint: 0.2,1
        pos_hint: {"X":0, 'y':0}
        text: "passer "


    Button:            
        size_hint: 0.3,0.2
        pos_hint: {"right":0.5, 'top':0.5}
        text: "faire l'appel"
